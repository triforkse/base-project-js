{
  "name": "project-base-js",
  "version": "0.1.0",
  "description": "Project Base for JS",
  "main": "src/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "mkdir -p dist && babel src/app.js > dist/app.js",
    "test": "mocha --compilers js:babel-core/register -c --require ./test/setup.js --recursive",
    "test:watch": "npm test -- --watch --growl",
    "start:production": "node dist/app.js",
    "start:dev": "nodemon --exec babel-node src/app.js",
    "start": "concurrent 'npm run start:dev' 'npm run test:watch'",
    "lint": "eslint src"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-2": "^6.1.2",
    "chai": "^3.4.0",
    "concurrently": "^0.1.1",
    "css-loader": "^0.21.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "expect": "^1.12.2",
    "file-loader": "^0.8.4",
    "growl": "^1.8.1",
    "isparta": "^3.5.0",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "postcss": "^5.0.10",
    "postcss-loader": "^0.7.0",
    "precss": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "author": "Thomas Anagrius <thb@trifork.com>",
  "license": "LicenseRef-LICENSE.md",
  "dependencies": {
    "express": "^4.13.3",
    "passport": "^0.3.0",
    "passport-http": "^0.3.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "winston": "^2.1.0"
  }
}
