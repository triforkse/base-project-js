{
  "name": "hygnudge-client",
  "version": "0.1.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config=./webpack.config.js",
    "test": "mocha --compilers js:babel-core/register -c --require ./test/setup.js --recursive",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register -c --require ./test/setup.js --recursive",
    "test:integration": "npm run test -- test/integration",
    "test:unit": "npm run test -- test/unit",
    "test:watch": "npm run test:unit -- --watch --growl",
    "start:dev": "node dev-server.js",
    "start:production": "node prod-server.js",
    "start": "concurrently --prefix \"client:{index}\" \"npm run start:dev\" \"npm run test:watch\" \"npm run lint:watch\"",
    "lint:watch": "npm run lint -- -w --fix",
    "lint": "esw src",
    "pre-commit": "npm run lint && npm run test"
  },
  "dependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.4.0",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "csswring": "^4.0.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-watch": "^2.1.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "growl": "^1.8.1",
    "isparta": "^4.0.0",
    "jsdom": "^8.2.0",
    "lil-http": "^0.1.16",
    "mocha": "^2.3.3",
    "postcss": "^5.0.10",
    "postcss-loader": "^0.8.2",
    "precss": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sinon": "^1.17.3",
    "socket.io-client": "^1.4.5",
    "style-loader": "^0.13.0",
    "unused-files-webpack-plugin": "^2.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "arrow-body-style": 0,
      "max-len": 0,
      "no-confusing-arrow": 0,
      "prefer-template": 1
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]]
      }
    }
  },
  "devDependencies": {
    "glob": "^7.0.3",
    "istanbul": "^0.4.0",
    "postcss-simple-vars": "^1.2.0"
  }
}
